{
  "name": "calendar-event-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "nodemon ./src/index.ts",
    "dev": "nodemon --exec ts-node ./src/index.ts",
    "install-all": "npm install && cd ./frontend && npm install",
    "start-frontend": "cd ./frontend && npm start",
    "docker:rebuild": "docker-compose build --no-cache",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down --remove-orphans",
    "test": "mocha -r ts-node/register ./src/tests/*.test.js --timeout 15000 --exit",
    "test:frontend": "cd ./frontend && npm test",
    "test:all": "npm run test && npm run test:frontend",
    "lint": "eslint . '**/*.js'",
    "format": "prettier --write '**/*.{js,ts,tsx,json,css,md}'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "faker": "^6.6.6",
    "mongodb": "^6.3.0",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sinon": "^17.0.1",
    "swagger-ui-express": "^4.6.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-react": "^7.33.2",
    "faker": "^5.5.3",
    "jest": "^29.7.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.8",
    "sinon-chai": "^3.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
